{
  "id": null,
  "uid": "quarkus",
  "title": "Quarkus JVM Dashboard",
  "tags": ["quarkus", "jvm", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "links": [],
  "refresh": "5s",
  "time": { "from": "now-15m", "to": "now" },
  "templating": { "list": [] },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "JVM CPU Load (process)",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "base_cpu_processCpuLoad_percent", "legendFormat": "process", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "percentunit" }, "overrides": [] }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "System CPU Load",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "vendor_cpu_systemCpuLoad_percent", "legendFormat": "system", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "percentunit" }, "overrides": [] }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "System Load Average (1m)",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "base_cpu_systemLoadAverage", "legendFormat": "loadavg", "refId": "A" }
      ]
    },

    {
      "id": 4,
      "type": "timeseries",
      "title": "Heap Used (bytes)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "base_memory_usedHeap_bytes", "legendFormat": "used", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] }
    },

    {
      "id": 5,
      "type": "timeseries",
      "title": "GC Collections (rate/s)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "rate(base_gc_total[5m])", "legendFormat": "{{name}}", "refId": "A" }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "GC Time (seconds/s)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "rate(base_gc_time_total_seconds[5m])", "legendFormat": "{{name}}", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    },

    {
      "id": 7,
      "type": "timeseries",
      "title": "JVM Uptime",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "base_jvm_uptime_seconds", "legendFormat": "uptime", "refId": "A" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    },

    {
      "id": 8,
      "type": "timeseries",
      "title": "Non-Heap Memory Used / Committed (bytes)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "vendor_memory_usedNonHeap_bytes", "legendFormat": "used", "refId": "A" },
        { "expr": "vendor_memory_committedNonHeap_bytes", "legendFormat": "committed", "refId": "B" }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] }
    },

    {
      "id": 9,
      "type": "timeseries",
      "title": "OS Memory (free physical / free swap)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 40 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        { "expr": "vendor_memory_freePhysicalSize_bytes", "legendFormat": "free physical", "refId": "A" },
        { "expr": "vendor_memory_freeSwapSize_bytes", "legendFormat": "free swap", "refId": "B" }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] }
    }
  ]
}
